{
  "private": true,
  "name": "@libsync/prettier-config",
  "description": "Prettier configuration for libsync projects",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Marcos Fernandes",
    "url": "https://github.com/rnarcos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnarcos/libsync.git",
    "directory": "configs/prettier-config"
  },
  "type": "commonjs",
  "main": "./index.cjs",
  "scripts": {
    "format": "prettier --check \"./*.cjs\" --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write \"./*.cjs\" --ignore-path ../../.prettierignore",
    "lint": "eslint \"./*.cjs\"",
    "lint:fix": "eslint \"./*.cjs\" --fix"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/prettier": "^3.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "exports": {
    "./base": "./base.cjs",
    ".": "./index.cjs",
    "./web": "./web.cjs",
    "./package.json": "./package.json"
  }
}
